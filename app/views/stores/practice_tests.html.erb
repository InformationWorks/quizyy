<%= render 'store_header' %>

<div class="span12">
  <div class="span8">
   
    <% if @store_entities.count == 0 %>
    
    	<img src="/assets/coming_soon.png" />
    	
    <% else %>
      
      <%= render :partial => "category_topic_quiz_group_block", :locals => { :store_entities => @store_entities, :timed => false } %>
      
    <% end %>
  </div>
  <%= content_tag "div", id: "cart-wrapper",class: "span3 well", data: { cart: @cart.cart_items.to_json(:include => :quiz) } do%>
  <% end %>
</div>
