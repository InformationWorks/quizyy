
<%= render 'store_header' %>

<div id="content" class="span12">
  
  	
  <% if current_user.purchased_full_quizzes.count == 0 %>
	<%= render "full_quiz_pack_selection"%>
  <% end %>
	
  <%= render "timed_quiz_buy_extra"%>
  
  <% if current_user.purchased_full_quizzes.count != 0 %>
    <%= content_tag "div", id: "cart-wrapper",class: "span3 well", data: { cart: @cart.cart_items.to_json(:include => :quiz) } do%>
  	<% end %>
  <% end %>
  	
</div> 

	 