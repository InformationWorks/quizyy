<% content_for :admin_content do %>

<!-- Data block -->
<article class="data-block">
	<div class="data-container">
		<header>
			<ul class="data-header-actions">
				<%= link_to(new_admins_quiz_path,:class => "btn btn-primary") do %>
				 Create New Quiz
				<% end %>
			</ul>
		</header>
		<section class="tab-content">
			<!-- Tab #static -->
			<div class="tab-pane active" id="static">
				<%= render :partial => "quiz_table", :locals => { :table_type => "category" }  %>
			</div>	
		</section>
		<footer class="error">
			<div class="alert">
				<strong style="font-size: 12px;">Be careful when modifying an approved quiz!</strong>
			</div>
		</footer>
	</div>
</article>
<!-- /Data block -->

<br />

<% end %>

<%= render 'admins/admin_base' %>